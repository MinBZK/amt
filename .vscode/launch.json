{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tad - fastapi",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "justMyCode": false,
      "args": [ "--log-level", "warning" ,"tad.main:app"],
      "cwd": "${workspaceFolder}/",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "envFile": "${workspaceFolder}/.env.test"
    },
    {
      "name": "Project: tests",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "cwd": "${workspaceFolder}",
      "justMyCode": true,
      "args": []
    }
  ]
}
